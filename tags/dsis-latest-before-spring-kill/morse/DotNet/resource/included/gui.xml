<?xml version="1.0" encoding="utf-8"?>
<Resources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <GnuplotTemplates>gnuplottemplate\</GnuplotTemplates>
  <GnuplotExe>gnuplot\bin\pgnuplot.exe</GnuplotExe>
  <GnuplotParams>-noend {0} -</GnuplotParams>
  <GnuplotTemplate2D>
    <Header>plot</Header>
    <BodyTemplate>'$file$' with dots</BodyTemplate>
    <Delimiter>,</Delimiter>
  </GnuplotTemplate2D>
  <GnuplotTemplate3D>
    <Header>splot</Header>
    <BodyTemplate>'$file$' with dots</BodyTemplate>
    <Delimiter>,</Delimiter>
  </GnuplotTemplate3D>
  <FileCreateTemplate>{0}.{1}.system.data</FileCreateTemplate>
</Resources>