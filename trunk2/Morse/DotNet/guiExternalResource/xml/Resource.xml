<?xml version="1.0" encoding="utf-8" ?> 


<resources>

  <strings>
 	<string key="data">value</string>
  </strings>	
  
  
  <xmls>
	<gnuplot>		
		<show>
			<exe>gnuplot\bin\pgnuplot.exe</exe>
			<params>-noend {0} -</params>
			<templates>
				<template dimension="2">
						<header>set title "$global_title$";  set key off; </header> 
						<history>set key below;</history>
						<plot>plot</plot>
						<bodyTemplate>'$file$' title "$title$" with dots</bodyTemplate>
						<delimiter>,</delimiter>
				</template>
				<template dimension="3">
						<header>set title "$global_title$";  set key off; </header>
						<history>set key below;</history>
						<plot> splot</plot>
						<bodyTemplate>'$file$' title "$title$" with dots</bodyTemplate>
						<delimiter>,</delimiter>
				</template>
			</templates>
		</show>
		<save>
		    <exe>gnuplot\bin\pgnuplot.exe</exe>
			<params> {0}</params>
			<templates>
				<template dimension="2">
					    <header>set title "$global_title$"; set terminal png size $width$,$height$; set output '$filename$'; set key off; </header>
					    <history>set key below;</history>
					    <plot>plot</plot>
					    <bodyTemplate>'$file$' title "$title$" with dots</bodyTemplate>
  					    <delimiter>,</delimiter>				
				</template>
				<template dimension="3">
						<header>set title "$global_title$"; set terminal png size $width$,$height$; set output '$filename$';  set key off; </header>
						<history>set key below; </history>
						<plot>splot</plot>
						<bodyTemplate>'$file$' title "$title$" with dots</bodyTemplate>
						<delimiter>,</delimiter>
				</template>
			</templates>
		</save>		
	</gnuplot>  
  </xmls>

</resources>